{
"extDescription": {
      "message": "The extension adds a handy list of notes that synchronizes with Google Bookmarks. A text selected on a web-page can be saved in the notes with a context menu command. The note is saved along with the web-page address from which was excerpted."
  },
"add_note": {
    "message": "New Note",
    "description": "Button adds new note"
  },
"remove_note": {
    "message": "Delete Note",
    "description": "Button deletes selected note"
  },
"options": {
    "message": "Options",
    "description": "Button shows page with options"
  },
"sync": {
    "message": "Synchronize with Google Bookmarks",
    "description": "Button for synchronization with Google bookmarks"
  },
"copy_to_note": {
    "message": "Copy to Note",
    "description": "Menu item thats copy selected text to notes"
  },
"title_date": {
    "message": "Last modified: $date$",
    "description": "Last modified date tooltip",
    "placeholders": {
      "date": {
        "content": "$1",
        "example": "12 jul 2010"
      }
    }
},
"title_url": {
    "message": "From: $url$",
    "description": "Note source URL tooltip",
    "placeholders": {
      "url": {
        "content": "$1",
        "example": "http://google.com/"
      }
    }
  },
"menu_copy":{
	"message": "Copy"
	},
"msg_need_login":{
	"message": "You should login first"
	},
"msg_sync_success":{
	"message": "Sync successfully finished"
	},
"msg_sync_error":{
	"message": "Sync error"
	},	
"msg_sync_with_errors":{
	"message": "Sync complete with errors"
	}

}